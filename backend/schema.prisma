// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider   = "prisma-client-js"
  output     = "node_modules/.prisma/client"
  engineType = "binary"
}

model User {
  id       String @id @default(cuid())
  name     String @default("")
  email    String @unique @default("")
  password String
}

model Catalogue {
  id            String      @id @default(cuid())
  name          String      @default("")
  catalogueItem Catalogue[] @relation("Catalogue_catalogueItem_Catalogue_catalogueItem")
  Catalogue     Catalogue?  @relation("Catalogue_catalogueItem_Catalogue_catalogueItem", fields: [catalogueId], references: [id])
  catalogueId   String?
}