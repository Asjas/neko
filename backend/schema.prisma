// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider   = "prisma-client-js"
  output     = "node_modules/.prisma/client"
  engineType = "binary"
}

model User {
  id       String  @id @default(cuid())
  name     String  @default("")
  email    String  @unique @default("")
  password String
  isAdmin  Boolean @default(false)
}

model Catalogue {
  id              String          @id @default(cuid())
  name            String          @default("")
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_mode      String?
  image_id        String?
  catalogueItem   CatalogueItem[] @relation("CatalogueItem_catalogue")
}

model CatalogueItem {
  id               String     @id @default(cuid())
  name             String     @default("")
  image_filesize   Int?
  image_extension  String?
  image_width      Int?
  image_height     Int?
  image_mode       String?
  image_id         String?
  martiniImageLink String     @default("")
  catalogue        Catalogue? @relation("CatalogueItem_catalogue", fields: [catalogueId], references: [id])
  catalogueId      String?    @map("catalogue")

  @@index([catalogueId])
}

model Technology {
  id     String @id @default(cuid())
  field1 String @default("")
  field2 String @default("")
  field3 String @default("")
  field4 String @default("")
}